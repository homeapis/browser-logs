{
  "name": "@homeapis/browser-logs",
  "version": "1.2.5",
  "description": "A React/Next.js SDK for logging from browsers",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "github",
    "url": "https://github.com/homeapis/browser-logs"
  },
  "scripts": {
    "build": "npx rimraf ./dist && tsc && tsc --project tsconfig.cjs.json",
    "test": "jest",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logging",
    "nextjs",
    "react",
    "sdk"
  ],
  "author": "HomeAPIs.com contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
